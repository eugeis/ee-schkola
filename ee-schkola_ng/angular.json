{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
        "sb-admin-cli-update": {
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/sb-admin-cli-update",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/config"
                        ],
                        "styles": [
                            "src/styles.sass"
                        ],
                        "scripts": [
                            "node_modules/@webcomponents/custom-elements/custom-elements.min.js"
                        ],
                        "allowedCommonJsDependencies": [
                            "@stomp/rx-stomp"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "simon-frontend:build"
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "sb-admin-cli-update:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "sb-admin-cli-update:serve:production"
                        },
                        "integration": {
                            "protractorConfig": "tests/protractor.conf.js"
                        }
                    }
                },
                "root": ""
            }
        }
    },
    "defaultProject": "sb-admin-cli-update",
    "cli": {
        "packageManager": "yarn",
        "analytics": false
    }
}
